<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:system="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <system:Boolean x:Key="IsDarkMode">true</system:Boolean>
  
  <Color x:Key="BackgroundColor">#1A0B2C</Color>
  <Color x:Key="BackgroundColorPointerOver">#2A153F</Color>
  <Color x:Key="BackgroundColorPressed">#2A153F</Color>
  <Color x:Key="BackgroundColorDisabled">#1A0B2C</Color>

  <Color x:Key="ForegroundColor">#EDEDED</Color>
  <Color x:Key="ForegroundColorPointerOver">#FFFFFF</Color>
  <Color x:Key="ForegroundColorPressed">#FFFFFF</Color>
  <Color x:Key="ForegroundColorDisabled">#A39EB5</Color>

  <Color x:Key="BorderColor">#BFA0FF</Color>
  <Color x:Key="BorderColorPointerOver">#D9B3FF</Color>

  <Color x:Key="SystemAccentColor">#A366FF</Color>
  <Color x:Key="SystemAccentColorLight1">#C299FF</Color>
  <Color x:Key="SystemAccentColorDark1">#7D33CC</Color>

  <Color x:Key="NeutralAccentColor">#9990A8</Color>
  <Color x:Key="NeutralAccentColorPointerOver">#B399CC</Color>

  <Color x:Key="AccentColor1">#A366FF</Color>
  <Color x:Key="AccentColor2">#FF66D9</Color>
  <Color x:Key="AccentColor3">#F3CDFF</Color>

  <Color x:Key="TickLineColor">#7D5BA6</Color>
  <Color x:Key="BarNumberColor">#D0C6E0</Color>
  <Color x:Key="FinalPitchColor">#D0C6E0</Color>
  <Color x:Key="TrackBackgroundAltColor">#2F1A40</Color>

  <Color x:Key="WhiteKeyColorLeft">#8E33CC</Color>
  <Color x:Key="WhiteKeyColorRight">#B366FF</Color>
  <Color x:Key="WhiteKeyNameColor">#FFFFFF</Color>

  <Color x:Key="CenterKeyColorLeft">#8255C7</Color>
  <Color x:Key="CenterKeyColorRight">#C299FF</Color>
  <Color x:Key="CenterKeyNameColor">#FFFFFF</Color>

  <Color x:Key="BlackKeyColorLeft">Transparent</Color>
  <Color x:Key="BlackKeyColorRight">Transparent</Color>
  <Color x:Key="BlackKeyNameColor">#FFFFFF</Color>
</ResourceDictionary>
